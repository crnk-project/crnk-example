{
	"compilerOptions": {
		"baseUrl": "",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2017",
			"dom"
		],
		"mapRoot": "./",
		"module": "es2015",
		"moduleResolution": "node",
		"outDir": "../dist/out-tsc",
		"sourceMap": true,
		"skipLibCheck": true,
		"target": "es5",
		"typeRoots": [
			"../node_modules/@types"
		]
	},
	"exclude": [
		"test.ts",
		"**/*.spec.ts"
	]
}
